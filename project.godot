config_version=5

[application]

config/name="Board Game"
config/tags=PackedStringArray("demo")
run/main_scene="res://tests/map tiles/test_map.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

EventsTest="*res://tests/events_test.gd"
GlobalDataTest="*res://tests/global_data_test.gd"
InventoryManager="*res://tests/inventory_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=854
window/size/viewport_height=480
window/size/resizable=false
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://ui/cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/project_time_tracker/plugin.cfg")

[file_customization]

folder_colors={
